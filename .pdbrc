# Print a dictionary, sorted. %1 is the dict, %2 is the prefix for the names.
alias pi for k in %1.__dict__.keys(): print("%1.", k, "=", %1.__dict__[k])
alias ps pi self

# Next and list.
alias nl n;;l
alias m nl

# Step and list.
alias sl s;;l
